{
  "name": "react-eth-challenge",
  "version": "1.0.0",
  "description": "React Eth Challenge",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "export REACT_APP_API_URL=http://localhost:3000/data && webpack-dev-server --open --mode development",
    "start-prod": "node server.js",
    "server": "json-server data.json",
    "format": "prettier --write '{*.js,src/**/*.{js,jsx}}'",
    "lint": "eslint src/ --fix",
    "test": "jest",
    "sass": "sass --watch src/styles/components/App.scss src/styles/components/App.css",
    "heroku-postbuild": "npm run build",
    "//": "See https://stackoverflow.com/questions/70368760/react-uncaught-referenceerror-process-is-not-defined",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "author": "Carlos J. Ramirez <cramirez@mediabros.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "babel-loader": "8.2.5",
    "express": "^4.18.1",
    "html-loader": "3.1.2",
    "html-webpack-plugin": "5.5.0",
    "jest-fetch-mock": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "^5.0.1",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@testing-library/react": "^13.3.0",
    "css-loader": "6.7.1",
    "eslint": "8.19.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.30.1",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "json-server": "^0.17.0",
    "mini-css-extract-plugin": "2.6.1",
    "prettier": "2.7.1",
    "react-error-overlay": "^6.0.9",
    "sass": "^1.53.0",
    "style-loader": "3.3.1",
    "stylus": "0.58.1",
    "stylus-loader": "7.0.0"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "\\.(styl|css)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/__test__/setupTest.js"
    ],
    "testEnvironment": "jsdom"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
