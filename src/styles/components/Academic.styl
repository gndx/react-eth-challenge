.Academic
    padding: 30px
    background-color: aliceblue
    border-radius: 15px

.Academic-item h5, .Academic p
    color: #555

.Certificate-content
    display: grid
    grid-template-columns: 48% 48%
    grid-gap: 4%

.Certificate-item div, .Certificate-item p
    clear: both

.left
    float:left

.right
    float:right
