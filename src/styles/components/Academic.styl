.Academic   
  grid-area: academic;
  display flex
  flex-direction column
  border-radius 10px
  border 1px solid #e0dfdc
  box-shadow 0 1px 0px 0px #00000008;
  background-color #ffffff
  padding 25px

  &-title
    color #000
    font-size 25px
    font-weight bold
    margin-bottom 5px

  &-desc
    color #000
    font-size 16px


