.c-Experience
  article.Card
    background: #A7ADC6;
  ul
    list-style: none;
    @media (max-width: 992px)
      padding: 0px 10px;
    @media (min-width: 992px)
      display: grid;
      grid-template-columns: 50% 50%;
      gap: 10px;
    li
      @media (max-width: 992px)
        margin: 40px 0px;
      h3, h5
        margin: 0px;
        padding: 0px;
      p
       width: 90%;
       font-size: 16px;