.Header
    display: grid;
    grid-template-columns: repeat(5, 20%);
    justify-items: left
    align-items: center
    column-gap: 2rem;

.Header img 
    grid-column: 1/2;
    object-fit: cover;
    width: 100%;
    max-height: 100%;

.content 
    color: #4E565F;
    grid-column: 2/end;

.Header-title
    color: #11ABB0
