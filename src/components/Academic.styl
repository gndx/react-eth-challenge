p
  margin: 0

.Academic
  &-title
    background-color: var(--secondary)
    color: var(--contrast2);
    hr
      background-color: var(--contrast2); 
  &-item
    color: var(--gray2);
    padding-block: 1rem;
    display: flex;
    flex-direction: column;
    margin-block-end: 1rem;
    gap: 1rem;
    &-institue
      font: var(--headline3);
      color: var(--contrast2);
      text-decoration: none;
    &-period
      font: var(--body1bold);
